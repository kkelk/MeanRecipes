{
    "title": "{{title}}",

    "ingredients": [
        {% for amount, unit, ingredient in ingredients %}
        [{{amount}}, "{{unit}}", "{{ingredient}}"]{% if not loop.last %},{% endif %}
        {% endfor %}
    ],

    "method": [
        {% for step in method %}
            "{{step}}"{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}
